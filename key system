-- Define the correct key
local correctKey = "KEY_tester" -- Replace this with the actual key

-- Create Screen GUI
local ScreenGui = Instance.new("ScreenGui")
local Frame = Instance.new("Frame")
local TextBox = Instance.new("TextBox")
local Button = Instance.new("TextButton")
local StatusLabel = Instance.new("TextLabel")

-- Parent objects
ScreenGui.Parent = game.CoreGui -- Make sure it runs in CoreGui to display properly
Frame.Parent = ScreenGui
TextBox.Parent = Frame
Button.Parent = Frame
StatusLabel.Parent = Frame

-- Frame properties
Frame.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
Frame.Size = UDim2.new(0, 300, 0, 150)
Frame.Position = UDim2.new(0.5, -150, 0.5, -75)

-- TextBox properties
TextBox.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
TextBox.Size = UDim2.new(0, 200, 0, 50)
TextBox.Position = UDim2.new(0, 50, 0, 20)
TextBox.PlaceholderText = "Enter Key"
TextBox.Font = Enum.Font.SourceSans
TextBox.TextSize = 20
TextBox.Text = ""

-- Button properties
Button.BackgroundColor3 = Color3.fromRGB(50, 200, 50)
Button.Size = UDim2.new(0, 100, 0, 30)
Button.Position = UDim2.new(0.5, -50, 0, 80)
Button.Text = "Check Key"
Button.Font = Enum.Font.SourceSans
Button.TextSize = 18

-- StatusLabel properties
StatusLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
StatusLabel.Size = UDim2.new(0, 300, 0, 30)
StatusLabel.Position = UDim2.new(0, 0, 0, 120)
StatusLabel.Font = Enum.Font.SourceSans
StatusLabel.TextSize = 18
StatusLabel.Text = ""
StatusLabel.TextColor3 = Color3.fromRGB(255, 0, 0)

-- Function to check the key when button is clicked
Button.MouseButton1Click:Connect(function()
    local enteredKey = TextBox.Text

    if enteredKey == correctKey then
        StatusLabel.Text = "Key is correct!"
        StatusLabel.TextColor3 = Color3.fromRGB(0, 255, 0)

        -- Execute the script if key is correct
        loadstring(game:HttpGetAsync("https://raw.githubusercontent.com/jxckplayxz/ALLOCORD/refs/heads/main/ALLOCORD-gui"))()
    else
        StatusLabel.Text = "Incorrect key, try again."
        StatusLabel.TextColor3 = Color3.fromRGB(255, 0, 0)
    end
end)
